  "bugs": {
    "url": "https://github.com/ashpfander/AuraRadio/issues"
  },
  "homepage": "https://github.com/ashpfander/AuraRadio#readme",
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "dependencies": {
    "dotenv": "^16.4.5"
  }
}
